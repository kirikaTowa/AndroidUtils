<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HorizontalWheelView">
        <attr name="marksCount" format="integer"/>
        <attr name="normalColor" format="color"/>
        <attr name="activeColor" format="color"/>
        <attr name="showActiveRange" format="boolean"/>
        <attr name="endLock" format="boolean"/>
        <attr name="onlyPositiveValues" format="boolean"/>
        <attr name="snapToMarks" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="HeightMaxScrollView">
        <attr name="maxHeight" format="dimension" />
    </declare-styleable>


    <declare-styleable name="ProgressDigitalSeekBar">
        <attr name="sk_colorProgressBg" format="reference|color"/>
        <attr name="sk_colorProgressFg" format="reference|color"/>
        <attr name="sk_heightProgress" format="reference|dimension"/>
        <attr name="sk_colorRectBg" format="reference|color"/>
        <attr name="sk_radiusRect" format="reference|dimension"/>
        <attr name="sk_sizeText" format="reference|dimension"/>
        <attr name="sk_colorText" format="reference|color"/>
        <attr name="sk_marginTextV" format="reference|dimension"/>
        <attr name="sk_marginTextH" format="reference|dimension"/>
        <attr name="sk_canSeek" format="reference|boolean"/>
    </declare-styleable>


</resources>